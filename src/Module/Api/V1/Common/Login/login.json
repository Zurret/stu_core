{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://stu.wolvnet.de/v1/common/login/login.json#",
    "type": "object",
    "properties": {
        "username": {
            "type": "string",
            "minLength": 1,
            "maxLength": 60
        },
        "password": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60
        }
    },
    "required": ["username", "password"],
    "additionalProperties": false
}